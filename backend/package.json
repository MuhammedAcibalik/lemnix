{
  "name": "lemnix-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Enterprise-grade aluminum profile cutting optimization backend",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:status": "prisma migrate status",
    "db:migrate:reset": "prisma migrate reset",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "ts-node prisma/seed.ts",
    "db:up": "docker-compose up -d postgres pgbouncer",
    "db:down": "docker-compose down",
    "db:logs": "docker-compose logs -f postgres",
    "db:migrate:postgres": "cross-env DATABASE_URL=$POSTGRES_URL prisma migrate deploy",
    "db:studio:postgres": "cross-env DATABASE_URL=$POSTGRES_URL prisma studio",
    "db:seed:postgres": "cross-env DATABASE_URL=$POSTGRES_URL ts-node prisma/seed.ts",
    "migrate:sqlite-to-postgres": "ts-node scripts/migrate-to-postgres.ts",
    "migrate:json-to-postgres": "ts-node src/scripts/migrate-json-to-postgres.ts",
    "db:optimize": "psql -U lemnix_user -d lemnix_db -f scripts/optimize-postgres.sql",
    "db:backup": "powershell -ExecutionPolicy Bypass -File scripts/backup-postgres.ps1",
    "db:backup:full": "ts-node scripts/backup-scheduler.ts backup",
    "db:backup:test": "ts-node scripts/backup-scheduler.ts test",
    "db:backup:schedule": "ts-node scripts/backup-scheduler.ts schedule",
    "db:maintenance:weekly": "ts-node scripts/maintenance-tasks.ts weekly",
    "db:maintenance:monthly": "ts-node scripts/maintenance-tasks.ts monthly",
    "db:maintenance:vacuum": "ts-node scripts/maintenance-tasks.ts vacuum",
    "db:maintenance:metrics": "ts-node scripts/maintenance-tasks.ts metrics",
    "db:rls:enable": "psql -U lemnix_user -d lemnix_db -f prisma/migrations/add_row_level_security.sql",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/qrcode": "^1.5.5",
    "@types/socket.io": "^3.0.1",
    "@webgpu/types": "^0.1.66",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "prisma": "^5.22.0",
    "puppeteer": "^24.29.1",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^7.4.0",
    "socket.io": "^4.8.1",
    "supertest": "^7.0.0",
    "uuid": "^10.0.0",
    "web-vitals": "^5.1.0",
    "winston": "^3.18.3",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.0.0",
    "eslint": "^9.34.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "optimization",
    "aluminum",
    "cutting",
    "enterprise",
    "typescript",
    "express"
  ],
  "author": "Lemnix Team",
  "license": "MIT"
}
